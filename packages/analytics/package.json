{
  "name": "@mindfiredigital/pivothead-analytics",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "description": "Data visualization and analytics module for PivotHead - supports Chart.js, ECharts, Plotly, and D3 (install your preferred library separately)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "postinstall.mjs"
  ],
  "author": "Mindfiredigital",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mindfiredigital/PivotHead.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "pivothead",
    "analytics",
    "data-visualization",
    "charts",
    "chart.js",
    "echarts",
    "plotly",
    "d3",
    "pivot-table",
    "business-intelligence",
    "data-analysis",
    "bar-chart",
    "pie-chart",
    "line-chart",
    "area-chart",
    "heatmap",
    "scatter-plot",
    "histogram",
    "treemap",
    "sankey",
    "funnel",
    "export",
    "pdf",
    "csv"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "postinstall": "node ./postinstall.mjs"
  },
  "dependencies": {
    "@mindfiredigital/pivothead": "workspace:*"
  },
  "peerDependencies": {
    "chart.js": "^4.4.3",
    "echarts": "^5.5.0",
    "plotly.js-dist": "^2.30.0",
    "d3": "^7.9.0",
    "jspdf": "^2.5.0"
  },
  "peerDependenciesMeta": {
    "chart.js": {
      "optional": true
    },
    "echarts": {
      "optional": true
    },
    "plotly.js-dist": {
      "optional": true
    },
    "d3": {
      "optional": true
    },
    "jspdf": {
      "optional": true
    }
  },
  "devDependencies": {
    "chart.js": "^4.4.3",
    "jspdf": "^2.5.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0"
  }
}
