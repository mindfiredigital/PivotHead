{
  "name": "@mindfiredigital/pivothead-vue",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@mindfiredigital/pivothead-web-component": "workspace:*"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.0",
    "@vitest/coverage-v8": "^0.34.0",
    "jsdom": "^22.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0",
    "vue": "^3.3.0"
  }
}
